{
  "expo": {
    "name": "Trashbusters",
    "slug": "trashbusters",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/locations_icon.png",
    "splash": {
      "image": "./assets/locations_splash_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "dev.emerald-river.trashbusters",
      "buildNumber": "0.1.0",
      "supportsTablet": true
    },
    "android": {
      "package": "dev.emerald_river.trashbusters",
      "versionCode": 0,
      "adaptiveIcon": {
        "foregroundImage": "./assets/locations_icon.png",
        "backgroundColor": "#171717"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA"
      ],
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
